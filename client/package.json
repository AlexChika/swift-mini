{
  "name": "swift-mini",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "dev": "next dev --turbo",
    "build": "next build",
    "deploy": "npm run typegen && npm run prisma && next build && npm run copy-Prisma-binary",
    "copy-Prisma-binary": "cp ./prisma/swift/libquery_engine-rhel-openssl-3.0.x.so.node ./.next/server/",
    "_": "",
    "prisma": "prisma generate",
    "analyze": "ANALYZE=true npm run build",
    "next:typegen": "next typegen",
    "chakra:typegen": "chakra typegen ./chakra/theme.ts",
    "typegen": "npm run next:typegen && npm run chakra:typegen",
    "__": "",
    "lint": "eslint .",
    "types:check": "tsc --noEmit",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@apollo/client": "^4.0.5",
    "@auth/prisma-adapter": "^2.10.0",
    "@chakra-ui/react": "^3.27.0",
    "@emotion/react": "^11.14.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@prisma/client": "^6.15.0",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "next": "^15.5.3",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-virtuoso": "^4.14.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^3.16.1",
    "@eslint/js": "^9.35.0",
    "@next/eslint-plugin-next": "^15.5.3",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.15.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
